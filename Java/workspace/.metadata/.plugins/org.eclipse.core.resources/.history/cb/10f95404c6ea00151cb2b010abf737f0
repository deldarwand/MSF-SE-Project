package pl.googlecardboard;

import java.io.IOException;

import pl.packet.Packet;

public final class HumidityPacket extends Packet{
	
	private int humidity;
	
	public void write(String input){
		this.humidity = Integer.parseInt(input);	
	}
	
	public byte[] read(){
		return new String("H" + Packet.DELIMETER + humidity + Packet.DELIMETER).getBytes();
	}
	
	public int getHumidity(){
		return humidity;
	}
}
