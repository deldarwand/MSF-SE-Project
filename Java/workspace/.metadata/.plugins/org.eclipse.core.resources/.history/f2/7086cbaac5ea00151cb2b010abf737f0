package pl.googlecardboard;

import pl.packet.Packet;

public final class TemperaturePacket extends Packet{

	private int temperature;
	
	public void write(String input){
		readLine(input);
		this.temperature = Integer.parseInt(readLine(input));
	}
	
	public byte[] read(){
		return new String("T" + Packet.DELIMETER + temperature + Packet.DELIMETER).getBytes();
	}
	
	public int getTemperature(){
		return temperature;
	}
}
