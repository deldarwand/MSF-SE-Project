package bpl.googlecardboard;

import bpl.packet.Packet;

public class TemperaturePacket {

	private int temperature;
	
	public void write(Object input){
		if(input instanceof Integer){
			this.temperature = ((Integer) input).intValue();
		}
	}
	
	public byte[] read(){
		return new String("T:" + temperature + Packet.DELIMETER).getBytes();
	}
}
